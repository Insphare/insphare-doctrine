{
  "name": "insphare/orm",
  "type": "library",
  "description": "Insphare orm simplified usage",
  "homepage": "https://github.com/Insphare/insphare-orm/",
  "keywords": [
    "insphare",
    "orm"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Will",
      "email": "info@insphare.de"
    }
  ],
  "require-dev": {
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Insphare/insphare-base"
    }
  ],
  "require": {
    "php": "7.4.3",
    "doctrine/orm": "v2.7.5",
    "symfony/yaml": "4.4.43",
    "insphare/base": "*",
    "beberlei/doctrineextensions": "^1.0"
  },
  "autoload": {
    "files": [
      "bootstrap.php"
    ]
  },
  "config": {
    "secure-http": true,
    "github-protocols": [
      "git",
      "https"
    ],
    "platform": {
      "php": "7.4.3"
    },
    "allow-plugins": {
      "ocramius/package-versions": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
